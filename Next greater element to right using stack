                long ans[]=new long[n];
        Stack<Long> st=new Stack<>();
        for(int i=n-1;i>=0;i--){
            if(st.isEmpty()) ans[i]=-1;
            while(!st.isEmpty() && st.peek()<=arr[i]) {
            st.pop();
            }
            
              if(st.isEmpty()) ans[i]=-1;
                else if(st.peek()>arr[i]) ans[i]=st.peek();
                
            
            st.push(arr[i]);
        }
        return ans;
        
    } 
